{
  "name": "discord-assistant",
  "version": "1.0.0",
  "description": "Advanced Discord bot using Sapphire framework",
  "main": "src/index.ts",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nodemon --watch src --exec \"ts-node -r tsconfig-paths/register\" src/index.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && tsc-esm-fix dist",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "sapphire",
    "typescript"
  ],
  "dependencies": {
    "@sapphire/framework": "^5.3.6",
    "@sapphire/plugin-api": "^8.3.1",
    "@sapphire/plugin-logger": "^4.1.0",
    "@sapphire/plugin-subcommands": "^7.0.1",
    "@sapphire/utilities": "^3.18.2",
    "@types/redis": "^4.0.11",
    "axios": "^1.11.0",
    "cron": "^4.3.2",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.0",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@types/cron": "^2.4.3",
    "@types/node": "^24.1.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsc-alias": "^1.8.16",
    "tsc-esm-fix": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
